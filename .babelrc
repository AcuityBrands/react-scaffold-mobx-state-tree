{
  "presets": [
    "react",
    ["env", {
      "targets": {
        "browsers": ["last 2 versions", "safari > 8", "not ie < 11"]
      },
      "modules": false
    }]
  ],
  "plugins": [
    "babel-plugin-syntax-dynamic-import",
    "babel-plugin-syntax-async-functions",
    "babel-plugin-transform-regenerator",
    ["babel-plugin-import", {"libraryName": "antd"}]
  ]
}